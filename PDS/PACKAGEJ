//* (userid).CBTxxx.FILE600.PDS(PACKAGEJ)
//* PACKAGE DATASETS FOR CBT
//*
//*
//DEL1     EXEC PGM=IDCAMS
//SYSPRINT DD  SYSOUT=*
//SYSIN    DD  *
 DEL 'AWCPAW.XMITA'
 DEL 'AWCPAW.XMITC'
 DEL 'AWCPAW.XMITO'
 DEL 'AWCPAW.XMITR'
 DEL 'AWCPAW.CBT.FILE600.XMIT'
 SET MAXCC=0
//*
//TSO1     EXEC PGM=IKJEFT1B,DYNAMNBR=500,COND=(0,NE)
//SYSTSPRT DD  SYSOUT=*
//SYSTSIN  DD  *
 XMIT SAOTEST.AWCPAW DS('AWCPAW.CBT.FILE600.ASM.PDS') +
  OUTDS('AWCPAW.XMITA')
 XMIT SAOTEST.AWCPAW DS('AWCPAW.CBT.FILE600.C.PDS') +
  OUTDS('AWCPAW.XMITC')
 XMIT SAOTEST.AWCPAW DS('AWCPAW.CBT.FILE600.OBJ.PDS') +
  OUTDS('AWCPAW.XMITO')
 XMIT SAOTEST.AWCPAW DS('AWCPAW.CBT.FILE600.REXX.PDS') +
  OUTDS('AWCPAW.XMITR')
//*
//FTP1     EXEC PGM=FTP,DYNAMNBR=500,COND=(0,NE),
//          PARM='-v -e -i bc331172'
//SYSPRINT DD  SYSOUT=*
//OUTPUT   DD  SYSOUT=*
//NETRC    DD  DISP=SHR,DSN=AWCPAW.NETRC
//INPUT    DD  *
 sendsite
 bin
 cd /D/Documents and Settings/wellspa/My Documents/mf/cbt/pw
 lcd 'AWCPAW.CBT.FILE600.PDS'
 get FTPCSMF.pdf $DOCFTPC (REPLACE
 cd /c/temp
 put 'AWCPAW.XMITA' XMITPDSA.xmi
 put 'AWCPAW.XMITC' XMITPDSC.xmi
 put 'AWCPAW.XMITO' XMITPDSO.xmi
 put 'AWCPAW.XMITR' XMITPDSR.xmi
 get XMITPDSA.xmi XMITPDSA (REPLACE
 get XMITPDSC.xmi XMITPDSC (REPLACE
 get XMITPDSO.xmi XMITPDSO (REPLACE
 get XMITPDSR.xmi XMITPDSR (REPLACE
 quit
//*
//TSO2     EXEC PGM=IKJEFT1B,DYNAMNBR=500,COND=(0,NE)
//SYSTSPRT DD  SYSOUT=*
//SYSTSIN  DD  *
 DEL 'AWCPAW.XMITA'
 DEL 'AWCPAW.XMITC'
 DEL 'AWCPAW.XMITO'
 DEL 'AWCPAW.XMITR'
 XMIT SAOTEST.AWCPAW DS('AWCPAW.CBT.FILE600.PDS') +
  OUTDS('AWCPAW.CBT.FILE600.XMIT')
//*
//FTP2     EXEC PGM=FTP,DYNAMNBR=500,COND=(0,NE),
//          PARM='-v -e -i bc331172'
//SYSPRINT DD  SYSOUT=*
//OUTPUT   DD  SYSOUT=*
//NETRC    DD  DISP=SHR,DSN=AWCPAW.NETRC
//INPUT    DD  *
 sendsite
 bin
 cd /D/Documents and Settings/wellspa/My Documents/mf/cbt/pw
 put 'AWCPAW.CBT.FILE600.XMIT' FILE600.XMI
 quit
//*
//TSO3     EXEC PGM=IKJEFT1B,DYNAMNBR=500,COND=(0,NE)
//SYSTSPRT DD  SYSOUT=*
//SYSTSIN  DD  *
 DEL 'AWCPAW.CBT.FILE600.XMIT'
//*
//CMPRS    EXEC PGM=IEBCOPY,COND=(0,NE)
//I1       DD  DISP=SHR,DSN=AWCPAW.CBT.FILE600.PDS
//I2       DD  DISP=SHR,DSN=AWCPAW.CBT.FILE600.ASM.PDS
//I3       DD  DISP=SHR,DSN=AWCPAW.CBT.FILE600.C.PDS
//I4       DD  DISP=SHR,DSN=AWCPAW.CBT.FILE600.OBJ.PDS
//I5       DD  DISP=SHR,DSN=AWCPAW.CBT.FILE600.REXX.PDS
//I6       DD  DISP=SHR,DSN=AWCPAW.CBT.FILE600.LOAD
//SYSPRINT DD  SYSOUT=*
//SYSIN    DD  *
 COPY I=I1,O=I1,LIST=NO
 COPY I=I2,O=I2,LIST=NO
 COPY I=I3,O=I3,LIST=NO
 COPY I=I4,O=I4,LIST=NO
 COPY I=I5,O=I5,LIST=NO
 COPY I=I6,O=I6,LIST=NO
